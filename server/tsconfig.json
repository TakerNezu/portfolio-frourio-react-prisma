{
  "compilerOptions": {
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "lib": ["dom"],
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "$/*": ["./*"],
      "$prisma/*": ["./node_modules/.prisma/*"]
    },
    "rootDir": ".",
    "skipLibCheck": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "target": "es6"
  }
}
